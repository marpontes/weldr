    public BufferedImage getScaledInstance(BufferedImage img,
                                           int targetWidth,
                                           int targetHeight,
                                           Object hint,
                                           boolean higherQuality)
    {
        int type = (img.getTransparency() == Transparency.OPAQUE) ?
            BufferedImage.TYPE_INT_RGB : BufferedImage.TYPE_INT_ARGB;
        BufferedImage ret = (BufferedImage)img;
        int w, h;
        if (higherQuality) {
            // Use multi-step technique: start with original size, then
            // scale down in multiple passes with drawImage()
            // until the target size is reached
            w = img.getWidth();
            h = img.getHeight();
        } else {
            // Use one-step technique: scale directly from original
            // size to target size with a single drawImage() call
            w = targetWidth;
            h = targetHeight;
        }
        
        do {
            if (higherQuality && w > targetWidth) {
                w /= 2;
                if (w < targetWidth) {
                    w = targetWidth;
                }
            }

            if (higherQuality && h > targetHeight) {
                h /= 2;
                if (h < targetHeight) {
                    h = targetHeight;
                }
            }

            BufferedImage tmp = new BufferedImage(w, h, type);
            Graphics2D g2 = tmp.createGraphics();
            g2.setRenderingHint(RenderingHints.KEY_INTERPOLATION, hint);
            g2.drawImage(ret, 0, 0, w, h, null);
            g2.dispose();
		
            ret = tmp;
        } while (w != targetWidth || h != targetHeight);

        return ret;
    }



public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    Object[] r = getRow();
 
    if (r == null) {
        setOutputDone();
        return false;
//		System.out.println("Output Done");
    }
 
    if (first){
        first = false;
    }
 
    r = createOutputRow(r, data.outputRowMeta.size());
     
    // Get the value from an input field
    String arquivo = get(Fields.In, "filename").getString(r);
    Double percentual = Double.valueOf(get(Fields.In, "percentual2").getString(r).replace(",","."));

     	

	BufferedImage resizeImageHintJpg = resizeImage(arquivo,percentual);
   	get(Fields.Out, "novo_nome").setValue(r, arquivo);
    
	System.out.println(arquivo);	
		
 
    // Send the row on to the next step.
    putRow(data.outputRowMeta, r);
 
    return true;
}





    public BufferedImage resizeImage(String originalImageURL, Double percent){
    	
    	
		try {
			
				BufferedImage originalImage = ImageIO.read(new File(originalImageURL));
				int type = originalImage.getType() == 0? BufferedImage.TYPE_INT_ARGB : originalImage.getType();
			
			
			
			
			
			 	final Double wid = originalImage.getWidth()*percent;
			    final Double hei = originalImage.getHeight()*percent; 
				
			 	

				Integer w = wid.intValue();
				Integer h = hei.intValue();
				
				
				//BufferedImage resizedImage = getScaledInstance(originalImage,w,h,RenderingHints.VALUE_INTERPOLATION_BICUBIC,true);
				


				BufferedImage resizedImage = new BufferedImage(w, h, type);
				Graphics2D g = resizedImage.createGraphics();
				

				g.drawImage(originalImage, 0, 0, w, h, null);
				g.dispose();	
				g.setComposite(AlphaComposite.Src);
			 
				g.setRenderingHint(RenderingHints.KEY_INTERPOLATION, RenderingHints.VALUE_INTERPOLATION_BICUBIC);
				g.setRenderingHint(RenderingHints.KEY_RENDERING, RenderingHints.VALUE_RENDER_QUALITY);
				g.setRenderingHint(RenderingHints.KEY_ANTIALIASING, RenderingHints.VALUE_ANTIALIAS_ON);
			 	 
				ImageIO.write(resizedImage, "jpg", new File(originalImageURL)); 
				return resizedImage;
			
			
		
		} catch (IOException e) {
			
			e.printStackTrace();
		}
		
    	
    	
   return null;
    }	
    
    
